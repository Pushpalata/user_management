<ul class="nav justify-content-center">
  <% if current_user %> 
    <% if current_user.admin? %>
      <li class="nav-item">
        <%= link_to "Users", users_profiles_path, class: "nav-link" %>
      </li>
    <% end %>
    <li class="nav-item">
      <%= link_to "My Profile", edit_users_profile_path(current_user.profile), class: "nav-link" %>
    </li>
    <li class="nav-item">
      <%= link_to "Sign Out", destroy_user_session_path, method: :delete, class: "nav-link" %>
    </li>
  <% else %>
    <li class="nav-item">
      <%= link_to "Sign In", new_user_session_path, class: "nav-link" %>
    </li>
    <li class="nav-item">
      <%= link_to  "Sign Up", new_user_registration_path, class: "nav-link" %>
    </li>
  <% end %> 
</ul>